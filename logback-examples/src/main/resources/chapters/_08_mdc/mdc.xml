<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
  <appender class="ch.qos.logback.core.ConsoleAppender"
    name="CONSOLE">
    <layout class="ch.qos.logback.classic.PatternLayout">
      <Pattern>%X{first} %X{last} - %m%n</Pattern>
    </layout>
  </appender>

  <root level="DEBUG">
    <appender-ref ref="CONSOLE"/>
  </root>
</configuration>